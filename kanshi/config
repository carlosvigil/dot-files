profile default {
	output eDP-1 disable
	output DP-1 enable mode 2560x1440@119.998Hz position 960,0 scale 1.5 transform normal
	output HDMI-A-2 enable mode 1920x1200@59.950Hz position 0,0 scale 1.25 transform 90
	exec swaymsg workspace 1, move workspace to DP-1
}

profile main {
	output eDP-1 disable
	output DP-1 enable mode 2560x1440@119.998Hz position 0,0 scale 1
	exec swaymsg workspace 1, move workspace to DP-1
}

profile second {
	output eDP-1 disable
	output HDMI-A-2 enable mode 1920x1200 position 0,0 scale 1 transform 90
	exec swaymsg workspace 1, move workspace to HDMI-A-2
}

profile base {
	output eDP-1 enable mode 2560x1600 position 0,0 scale 2
}
