profile default {
	output DP-1 position 1200,240
	output HDMI-A-2 position 0,0 transform 90
}

profile main {
	output eDP-1 disable
	output DP-1 enable mode 2560x1440@119.998Hz position 0,0 scale 1
}

profile second {
	output eDP-1 disable
	output HDMI-A-2 enable mode 1920x1200 position 0,0 scale 1 transform 90
}

profile base {
	output eDP-1 enable mode 2560x1600 position 0,0 scale 2
}
# rough position of laptop screen when 3 up, 960x1152
